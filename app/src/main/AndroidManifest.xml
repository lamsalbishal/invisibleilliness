<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.invisibleillnesses">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/invisibleillnesseslogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/invisibleillnesseslogo"
        android:supportsRtl="true"
        android:theme="@style/Theme.InvisibleIllnesses"
        tools:replace="android:theme"
        tools:targetApi="31">
        <activity
            android:name=".Admin.AddProductActivity"
            android:exported="false" />
        <activity
            android:name=".Admin.AdminProductActivity"
            android:exported="false" />
        <activity
            android:name=".Pages.AdminActivity"
            android:exported="false" />
        <activity
            android:name=".SplashScreenActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Pages.ForgetPasswordActivity"
            android:exported="false" />
        <activity
            android:name=".Pages.SignupActivity"
            android:exported="false" />
        <activity
            android:name=".Pages.SigninActivity"
            android:exported="true" />
        <activity
            android:name=".MainActivity"
            android:exported="true" />
        <activity
            android:name=".Signup"
            android:exported="true" />
    </application>

</manifest>